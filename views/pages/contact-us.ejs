<!DOCTYPE html>
<html>
    <head>
        <% include ../partials/header.ejs %>
        <link rel="stylesheet" type="text/css" href="/stylesheets/main.css" />
    </head>

    <body>
        
      <% include ../partials/nav.ejs %>  
      <div class="container" style="margin-top:100px">
          <div class="row">
            </div>
            <div class="col-sm-16">
              <h2 class="titles">Galeria</h2>
            <div id="demo" class="carousel slide" data-ride="carousel">

          <!-- Indicators -->
          <ul class="carousel-indicators">
            <li data-target="#demo" data-slide-to="0" class="active"></li>
            <li data-target="#demo" data-slide-to="1"></li>
            <li data-target="#demo" data-slide-to="2"></li>
          </ul>
        
          <!-- The slideshow -->
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img src="../galeria1.jpg" class="rounded" width="1000" height="600" alt="">
            </div>
            <div class="carousel-item">
              <img src="../galeria2.jpg" class="rounded" width="1000" height="600" alt="Chicago">
            </div>
            <div class="carousel-item">
              <img src="../galeria3.jpg" class="rounded" width="1000" height="600" alt="New York">
            </div>
            <div class="carousel-item">
                <img src="../galeria4.jpg" class="rounded" width="1000" height="600" alt="New York">
              </div>
          </div>
        
          <!-- Left and right controls -->
          <a class="carousel-control-prev" href="#demo" data-slide="prev">
            <span class="carousel-control-prev-icon"></span>
          </a>
          <a class="carousel-control-next" href="#demo" data-slide="next">
            <span class="carousel-control-next-icon"></span>
          </a>
        
        </div>
        </div>
      </div>
                  
      <% include ../partials/footer.ejs %>  
    </body>
</html>